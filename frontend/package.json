{
  "name": "edifice-react-boilerplate",
  "version": "1.0.0-develop.20230831021",
  "description": "Edifice React boilerplate",
  "homepage": "https://github.com/opendigitaleducation/edifice-react-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/opendigitaleducation/edifice-react-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendigitaleducation/edifice-react-boilerplate.git"
  },
  "license": "GPL-3.0",
  "author": "Edifice",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "concurrently \"pnpm:clean:*\"",
    "clean:dist": "rm -rf dist",
    "clean:lighthouse": "rm -rf .lighthouseci",
    "clean:modules": "rm -rf node_modules",
    "clean:package": "rm -f pnpm-lock.yaml",
    "dev": "vite",
    "fix": "eslint --fix .",
    "format": "pnpm format:write && pnpm format:check",
    "format:check": "npx prettier --check .",
    "format:write": "npx prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "lighthouse": "pnpm clean:lighthouse && lhci autorun",
    "lint": "eslint .",
    "pre-commit": "lint-staged",
    "prepare": "cd .. && husky install frontend/.husky",
    "preview": "vite preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm run format",
      "pnpm run fix"
    ]
  },
  "dependencies": {
    "@edifice-tiptap-extensions/extension-attachment": "develop-b2school",
    "@edifice-tiptap-extensions/extension-iframe": "develop-b2school",
    "@edifice-tiptap-extensions/extension-typosize": "develop-b2school",
    "@edifice-tiptap-extensions/extension-video": "develop-b2school",
    "@edifice-ui/icons": "develop-b2school",
    "@edifice-ui/react": "develop-b2school",
    "@react-spring/web": "9.7.3",
    "@tanstack/react-query": "4.32.6",
    "@tiptap-pro/extension-mathematics": "2.2.1",
    "@tiptap/core": "2.0.3",
    "@tiptap/extension-color": "2.0.3",
    "@tiptap/extension-font-family": "2.0.3",
    "@tiptap/extension-heading": "2.0.3",
    "@tiptap/extension-highlight": "2.0.3",
    "@tiptap/extension-image": "2.0.3",
    "@tiptap/extension-link": "2.0.3",
    "@tiptap/extension-list-item": "2.0.3",
    "@tiptap/extension-subscript": "2.0.3",
    "@tiptap/extension-superscript": "2.0.3",
    "@tiptap/extension-table": "2.0.3",
    "@tiptap/extension-table-cell": "2.0.3",
    "@tiptap/extension-table-header": "2.0.3",
    "@tiptap/extension-table-row": "2.0.3",
    "@tiptap/extension-text-align": "2.0.3",
    "@tiptap/extension-text-style": "2.0.3",
    "@tiptap/extension-typography": "2.0.3",
    "@tiptap/extension-underline": "2.0.3",
    "@tiptap/pm": "2.0.3",
    "@tiptap/react": "2.0.3",
    "@tiptap/starter-kit": "2.0.3",
    "edifice-ts-client": "develop-b2school",
    "emoji-picker-react": "^4.4.11",
    "i18next": "23.4.3",
    "i18next-http-backend": "2.2.1",
    "katex": "^0.16.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "react-hook-form": "7.45.4",
    "react-hot-toast": "2.4.1",
    "react-i18next": "13.0.3",
    "react-intersection-observer": "9.5.2",
    "swiper": "10.1.0",
    "zustand": "4.4.1"
  },
  "devDependencies": {
    "@axe-core/react": "4.7.3",
    "@tanstack/react-query-devtools": "4.32.6",
    "@types/node": "20.4.8",
